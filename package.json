{
  "name": "threejs-sketchfab-example",
  "version": "1.0.0",
  "main": "ATON.service.main.js",
  "license": "MIT",
  "scripts": {
    "dev": "parcel index.html",
    "build": "parcel build ./*.html",
    "build-three": "webpack --config ./services/tools/webpack.three.js",
    "build-aton": "webpack --config ./services/tools/webpack.aton.js",
    "start-nodemon": "nodemon services/Proxy.service.js",
    "start-aton": "webpack --config ./services/tools/webpack.aton.js && node services/ATON.service.main.js",
    "build-both": "webpack --config ./services/tools/webpack.three.js && webpack --config ./services/tools/webpack.aton.js && node services/ATON.service.main.js",
    "start-maton": "webpack --config ./services/tools/webpack.aton.js && node services/ATON.service.main.mjs",
    "start-proxy": "webpack --config ./services/tools/webpack.proxy.js && node Proxy.service.js",
    "serve": "parcel build ./**/*.html && webpack --config ./services/tools/webpack.aton.js && nodemon services/Proxy.service.js",
    "serve-aton": "parcel build ./public/**/**/*.html && webpack --config ./services/tools/webpack.aton.js && node services/ATON.service.main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-photon": "node services/photon/ATON.service.photon.js",
    "start-webdav": "node services/webdav/ATON.service.webdav.js",
    "deploy-pm2": "pm2 start ecosystem.config.js",
    "upd": "git pull && npm install",
    "first-setup": "npm install && npm run build-three && npm run build-aton",
    "build-apidoc-client": "jsdoc -c ./services/tools/jsdoc-client.json",
    "build-apidoc-server": "apidoc -c ./services/tools/apidoc-server.json -i ./services/ -o ./public/apidoc/server/ -e _prv -t ./_prv/apidoc-template/template",
    "build-aton-and-run": "npm run build-aton && npm start"
  },
